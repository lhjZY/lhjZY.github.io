{
  "name": "vitepress-theme-usgs",
  "version": "1.0.0",
  "description": "A minimalist VitePress theme inspired by U.S. Graphics Company design",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles": "./dist/styles/main.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "vite build --watch"
  },
  "keywords": [
    "vitepress",
    "theme",
    "blog",
    "minimalist",
    "usgs"
  ],
  "author": "huanjun",
  "license": "MIT",
  "peerDependencies": {
    "vitepress": "^1.0.0",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vue-tsc": "^2.0.0"
  },
  "dependencies": {
    "medium-zoom": "^1.1.0"
  }
}
